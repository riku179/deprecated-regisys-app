{
    "extends": "tslint:recommended",
    "rules": {
        "trailing-comma": [
            false,
            {
                "singleline": "never",
                "multiline": "always"
            }
        ],
        "semicolon": false,
        "no-console": false,
        "member-access": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name": false
    }
}
